    {% set first_area_color = node.field_first_content_area_bg.value %}

    {% set second_area_color = node.field_second_content_area_bg.value %}

    {% set third_area_color = node.field_third_content_area_bg_colo.value %}

    {% set fourth_area_color = node.field_fourth_content_area_bg_col.value %}

    {% set fifth_area_color = node.field_fifth_content_area_bg_colo.value %}

    {% set sixth_area_color = node.field_sixth_content_area_bg_colo.value %}


    {% set language = 'english' %}
    {% if page['primary_menu']['spanishmainnavigation'] is not empty %}
      {% set language = 'spanish' %}
    {% endif %}
    <div
      id="page-wrapper"
      class="{{language}} bg-color-{{header_area_color}}">
