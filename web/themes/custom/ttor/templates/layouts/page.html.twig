      height="70"
      viewbox="0 0 70 70">
      <defs>
        <clipPath
          id="clip-path">
          <rect
            id="Rectangle_299"
            data-name="Rectangle 299"
            width="35"
            height="28.395"
            fill="#003087"/>
        </clipPath>
